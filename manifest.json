{
"manifest_version": 2,
"name": "GlobalTab",
"description": "",
"version": "1.0",
"author": "Jennifer Yen",
"chrome_url_overrides" : {
  "newtab" : "newTab.html"
},

  "permissions": [
    "background",
    "tabs",
    "storage",
    "https://*/*",
    "http://*/*"
  ],

  "browser_action": {
    "default_title": "GlobalTab",
    "default_icon": {
      "16": "img/earth16.png",
      "32": "img/earth32.png",
      "48": "img/earth48.png",
      "128": "img/earth128.png"
    }
  },

  "icons": {
    "16": "img/earth16.png",
    "32": "img/earth32.png",
    "48": "img/earth48.png",
    "128": "img/earth128.png"
  }
}
